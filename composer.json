    "description": "Symfony 4 bundle providing out of the box support for trace id parameter injection to request/response/logs",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Simo Simeunovic",
            "email": "simo.simeunovic@esome.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "esome\\TraceIdBundle\\": "./src"
        }
    },
    "require": {
        "php": "^8.4",
        "symfony/http-foundation": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/config": "^7.2",
